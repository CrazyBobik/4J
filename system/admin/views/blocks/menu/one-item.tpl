<div class="main-menu-item" data-id="{id}" data-type="{type}">
    <div class="main-menu-btn {class}">
        <i class="fa {faIcon}"></i>
        <span>{title}</span>
    </div>
    {toggle}
    <aside class="main-menu-panel">
        <div class="panel-elem add-tree-leaf">
            <i class="fa fa-plus"></i>
        </div>
        <div class="panel-elem del-tree-leaf">
            <i class="fa fa-trash"></i>
        </div>
    </aside>
</div>
{subMenu}